#include<stdio.h>
void main()
{
int n1,n2,n3,n4;
printf("Enter n1:");
scanf("%d",&n1);
printf("Enter n2:");
scanf("%d",&n2);
printf("Enter n3:");
scanf("%d",&n3);
printf("Enter n4:");    
scanf("%d",&n4);
  if(n1>n2)
    { 
      if(n1>n3)
        {
         if(n1>n4)
            printf("%d is big",n1);
         else
            printf("%d is big",n4);
         }
      else 
        {
          if(n3>n4)
            printf("%d is big",n3):
          else
            printf("%d is big",n4);
        }
     }
   else
     {
      if(n2>n3)
        {
          if(n2>n4)
            printf("%d is big",n2);
          else
            printf("%d is big",n4);
        }
       else
        { 
           if(n3>n4)
             printf("%d is big",n3);
           else
             printf("%d is big",n4);
        }
      }
 }
         




